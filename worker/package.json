{
  "private": true,
  "name": "sharecation-worker",
  "version": "1.0.0",
  "description": "A template for kick starting a Cloudflare Workers project",
  "main": "index.mjs",
  "types": "module",
  "scripts": {
    "build": "tsc --noEmit",
    "build:emit": "tsc",
    "clean": "node clean.mjs",
    "format": "prettier --write '**/*.{ts,mjs,css,json,md}'",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "publish:developement": "node publish.mjs --environment=development",
    "publish:developement:ping": "node publish.mjs --environment=development --bins=ping",
    "publish:developement:images": "node publish.mjs --environment=development --bins=images",
    "publish:developement:groups": "node publish.mjs --environment=development --bins=groups",
    "publish:developement:authentication": "node publish.mjs --environment=development --bins=authentication",
    "publish:developement:authentication-scheduled": "node publish.mjs --environment=development --bins=authentication-scheduled",
    "publish:developement:dev-tools": "node publish.mjs --environment=development --bins=dev-tools",
    "publish:production": "node publish.mjs --environment=production"
  },
  "author": "donatowolfisberg <donato@wolfisberg.dev>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "3.8.0",
    "@miniflare/kv": "2.4.0",
    "@types/jest": "27.5.0",
    "esbuild": "0.14.38",
    "jest": "28.0.3",
    "jest-config": "28.0.3",
    "jest-environment-miniflare": "2.4.0",
    "miniflare": "2.4.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.1",
    "typescript": "4.6.4",
    "wrangler": "0.0.27"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "2.5.0",
    "workers-loki-logger": "0.1.9"
  }
}
